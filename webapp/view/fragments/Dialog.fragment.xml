<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Currency"
		items="{ path : 'helpModel>/', length : 300 }"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<StandardListItem
			title="{helpModel>Currency}"
			description="{helpModel>CurrencyName}" />
	</SelectDialog>
</core:FragmentDefinition>